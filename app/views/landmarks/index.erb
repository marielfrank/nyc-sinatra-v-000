<h1>Landmarks</h1>
<ul>
  <% @landmarks = Landmark.all.sort { |lm| lm.name } %>
  <% @landmarks.each do |landmark| %>
    <li>
      <a href="/landmarks/<%= landmark.id %>">
        Name: <%= landmark.name %><br>
        Year Completed: <%= landmark.year_completed %>
      </a>
    </li>
  <% end %>
</ul>
